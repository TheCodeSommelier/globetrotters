
<div class="row m-1">
    <div class="col-12 my-3">
      <%= render 'shared/search', url: users_path %>
    </div>
</div>

<div class="users-box">
  <% @users.each do |user| %>
    <div class="user-card">
    <%= link_to profile_page_path(user.username) do  %>
      <%= image_tag(user.photo) %>
      <% end %>
      <%= link_to profile_page_path(user.username) do  %>
        <p> <%= user.username %> </p>
      <% end %>
    </div>
  <% end %>
</div>
